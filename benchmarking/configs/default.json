{
    "Requests": 50000,
    "WarmUpRequests": 1000,
    "Concurrency": 16,
    "RequestTimeout": 2,
    "Host": "127.0.0.1",
    "Port": 8080,
    "KeepAlive": true,
    "Headers": {
        "Accept-Encoding": "gzip",
        "Accept-Type": "*/*"
    },
    "Configurations": ["Release"],
    "Frameworks": ["net461", "net6.0", "net8.0"],
    "Variations": [
        {
            "Name": "Minimal",
            "FileName": "minimal",
            "LogToConsole": true,
            "LogWithColors": false,
            "LogLevel": "Information",
            "Job": "Minimal",
            "Route": "/"
        },
        {
            "Name": "Infrastructure",
            "FileName": "infrastructure",
            "LogToConsole": true,
            "LogWithColors": false,
            "LogLevel": "Information",
            "Job": "Infrastructure",
            "Route": "/"
        }
    ]
}